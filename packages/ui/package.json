{
  "name": "@ktcloud-front/ui",
  "type": "module",
  "version": "0.1.1",
  "private": false,
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsc -b && vite build && storybook build",
    "build-storybook": "storybook build",
    "build-package": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "dependencies": {
    "@storybook/react-vite": "^8.3.4",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^8.56.0",
    "react": "18.3.1",
    "react-dom": "^18",
    "vite-plugin-commonjs": "^0.10.3",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.8.0",
    "@ktcloud-front/eslint-config": "*",
    "@ktcloud-front/typescript-config": "*",
    "@storybook/addon-essentials": "^8.3.2",
    "@storybook/addon-interactions": "^8.3.2",
    "@storybook/addon-links": "^8.3.2",
    "@storybook/addon-onboarding": "^8.3.2",
    "@storybook/blocks": "^8.3.2",
    "@storybook/manager-api": "^8.3.2",
    "@storybook/react": "^8.3.2",
    "@storybook/test": "^8.3.2",
    "@storybook/theming": "^8.3.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "8.56.0",
    "eslint-config-next": "14.2.8",
    "eslint-plugin-storybook": "^0.8.0",
    "storybook": "^8.3.3",
    "tslib": "^2.7.0",
    "typescript": "5.5.4",
    "vite-plugin-dts": "^4.2.2",
    "vite-plugin-lib-inject-css": "^2.1.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ktcloud-front/front"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
